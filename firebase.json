{
  "hosting": {
    "public": "build",
    "ignore": ["firebase.json", "**/.*", "**/node_modules/**"],
    "rewrites": [
      {
        "source": "!/static/**",
        "destination": "/index.html"
      },
      {
        "source": "!/logo2.jpg",
        "destination": "/index.html"
      },
      {
        "source": "!/asset-manifest.json",
        "destination": "/index.html"
      },
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  }
}
